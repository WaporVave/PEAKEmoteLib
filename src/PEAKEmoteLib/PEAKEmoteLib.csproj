<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <AssemblyName>com.github.WaporVave.PEAKEmoteLib</AssemblyName>
    <AssemblyTitle>PEAKEmoteLib</AssemblyTitle>
    <Version>0.2.0</Version>

    <PackageId>PEAKEmoteLib</PackageId>
    <Authors>WaporVave</Authors>
    <Description>An API for custom emotes in the game PEAK</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/WaporVave/PEAKEmoteLib</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="PEAKModding.PEAKLib.UI" Version="1.6.0" Private="False"/>
  </ItemGroup>

</Project>
